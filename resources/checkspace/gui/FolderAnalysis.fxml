<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<HBox
  xmlns:fx="http://javafx.com/fxml"
  fx:controller="checkspace.analysis.FolderAnalysisController">
  <TextField fx:id="folderPathTextField"/>
  <Button
    fx:id="startButton"
    text="%startFolderAnalysis.buttonText"
    onAction="#startFolderAnalysis"/>
  <Button
    fx:id="cancelButton"
    text="%cancelFolderAnalysis.buttonText"
    onAction="#cancelFolderAnalysis"/>
</HBox>
